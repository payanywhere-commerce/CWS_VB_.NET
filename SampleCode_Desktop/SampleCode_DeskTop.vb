        Private Sub cmdAuthorizeAndCapture_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdAuthorizeAndCapture.Click
            'The AuthorizeAndCapture() operation is used to authorize and capture a transaction in a single invocation.
            Cursor = Cursors.WaitCursor

            'Check to see if this transaction type is supported
            If Not SupportedTxnTypes.AuthAndCapture Then
                MessageBox.Show("AuthAndCapture Not Supported")
                Cursor = Cursors.[Default]
                Return
            End If

            If _bcs IsNot Nothing Then
                If Not chkProcessAsPINDebitTxn.Checked AndAlso _bcs.Tenders.CreditAuthorizeSupport = CreditAuthorizeSupportType.Both Then
                    If _bcs.Tenders.CreditAuthorizeSupport = CreditAuthorizeSupportType.AuthorizeOnly Then
                        MessageBox.Show("This service only support AuthorizeOnly for Credit transactions")
                        Cursor = Cursors.[Default]
                        Return
                    End If
                    If _bcs.Tenders.CreditAuthorizeSupport = CreditAuthorizeSupportType.AuthorizeAndCaptureOnly Then
                        MessageBox.Show("This service only support AuthorizeAndCaptureOnly for Credit transactions")
                        Cursor = Cursors.[Default]
                        Return
                    End If
                End If

                Try
                    Dim BCtransaction As BankcardTransaction = SetBankCardTxnData()
                    Helper.ProcessBCPTransaction(TransactionType.AuthorizeAndCapture, BCtransaction, Nothing, Nothing, Nothing, Nothing, _
      Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            ElseIf _ecks IsNot Nothing Then
                'Process as a Check transaction
                Try
                    MessageBox.Show("Placeholder for ECK code. Please ask your solution consultant for an example")
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            ElseIf _svas IsNot Nothing Then
                'Process as a Check transaction
                Try
                    Dim SVATransaction As StoredValueTransaction = SetStoredValueTxnData()

                    Dim response As New List(Of Response)()
                    'Let's Query a transaction
                    response = Helper.ProcessSVATransaction(TransactionType.Authorize, SVATransaction, Nothing, Nothing, Nothing, Nothing, _
                     ChkAcknowledge.Checked)
                    If response.Count > 0 Then
                        Dim SVA As StoredValueTransactionResponse = DirectCast(response(0), StoredValueTransactionResponse)
                        If SVA.Status = Status.Successful Then
                            'In this case the check is ACHCapable meaning that it can now be processed as a check
                            _authorizeTxn = SVA.TransactionId
                        End If
                    End If
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            End If
        End Sub

        Private Sub cmdAuthorize_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdAuthorize.Click
            'The Authorize() operation is used to authorize transactions prior to capture.
            Cursor = Cursors.WaitCursor

            'Check to see if this transaction type is supported
            If Not SupportedTxnTypes.Authorize Then
                MessageBox.Show("Authorize Not Supported")
                Return
            End If
            'Check to see if the transaction is a PINDebit and that the AuthorizeAndCapture is used.
            If chkProcessAsPINDebitTxn.Checked Then
                MessageBox.Show("A PINDebit transaction can only be processed as an AuthorizeAndCapture Transaction")
                Cursor = Cursors.[Default]
                Return
            End If

            If _bcs IsNot Nothing Then
                'Process a BankCard Transaction
                Try
                    Dim BCtransaction As BankcardTransaction = SetBankCardTxnData()
                    Dim response As New List(Of Response)()
                    response = Helper.ProcessBCPTransaction(TransactionType.Authorize, BCtransaction, Nothing, Nothing, Nothing, Nothing, _
      Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                    If response.Count < 1 Then
                        Return
                    End If
                    Dim BCR As BankcardTransactionResponse = DirectCast(response(0), BankcardTransactionResponse)
                    'Queue up an authorize for a capture
                    _authorizeTxn = BCR.TransactionId
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]

                End Try
            ElseIf _ecks IsNot Nothing Then
                'Process as a Check transaction
                Try
                    If _queryAccountTxn Is Nothing Then
                        MessageBox.Show("You must first perform a QuearyAccount() to verify that the account accepts ACH transactions")
                        Return
                    End If
                    If Not _queryAccountTxn.ACHCapable Then
                        MessageBox.Show("Your previous QueryAccount() returned a response where the transaction is not ACHCapable")
                        Return
                    End If

                    Dim ECKTransaction As ElectronicCheckingTransaction = SetElectronicCheckTxnData()

                    'If a modified account or routing number is returned, those numbers should be used for the check transaction.  
                    'Good for verifying the info before running the charge  like when you initially set-up a recurring payment.
                    If ECKTransaction.TenderData.CheckData.AccountNumber <> _queryAccountTxn.ModifiedAccountNumber Then
                        ECKTransaction.TenderData.CheckData.AccountNumber = _queryAccountTxn.ModifiedAccountNumber
                    End If
                    If ECKTransaction.TenderData.CheckData.RoutingNumber <> _queryAccountTxn.ModifiedRoutingNumber Then
                        ECKTransaction.TenderData.CheckData.RoutingNumber = _queryAccountTxn.ModifiedRoutingNumber
                    End If

                    _queryAccountTxn = Nothing
                    'Clear out the value as we're going to process a Check
                    Dim response As New List(Of Response)()
                    'Let's Query a transaction
                    response = Helper.ProcessECKTransaction(TransactionType.Authorize, ECKTransaction, Nothing, Nothing, Nothing, ChkAcknowledge.Checked)
                    If response.Count > 0 Then
                        Dim ECK As ElectronicCheckingTransactionResponse = DirectCast(response(0), ElectronicCheckingTransactionResponse)
                        If ECK.Status = Status.Successful Then
                            'In this case the check is ACHCapable meaning that it can now be processed as a check
                            _checkTxn = ECK.TransactionId
                        End If
                    End If
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            ElseIf _svas IsNot Nothing Then
                'Process as a Check transaction
                Try
                    Dim SVATransaction As StoredValueTransaction = SetStoredValueTxnData()

                    Dim response As New List(Of Response)()
                    'Let's Query a transaction
                    response = Helper.ProcessSVATransaction(TransactionType.Authorize, SVATransaction, Nothing, Nothing, Nothing, Nothing, _
                     ChkAcknowledge.Checked)
                    If response.Count > 0 Then
                        Dim SVA As StoredValueTransactionResponse = DirectCast(response(0), StoredValueTransactionResponse)
                        If SVA.Status = Status.Successful Then
                            _authorizeTxn = SVA.TransactionId
                        End If
                    End If
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]

                End Try
            End If
        End Sub

        Private Sub cmdCapture_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdCapture.Click
            'The Capture() operation is used to capture a single transaction after it has been successfully authorized.
            Cursor = Cursors.WaitCursor

            'Check to see if this transaction type is supported
            If Not SupportedTxnTypes.Capture Then
                MessageBox.Show("Capture Not Supported")
                Cursor = Cursors.[Default]
                Return
            End If
            If _authorizeTxn.Length < 1 Then
                MessageBox.Show("Please send an Authorize before sending the Capture")
                Cursor = Cursors.[Default]
                Return
            End If

            If _bcs IsNot Nothing Then
                'Process a BankCard Transaction
                Try
                    Dim BCDifferenceData As New BankcardCapturePro()
                    Dim strAuthTxn As String = _authorizeTxn
                    _authorizeTxn = ""
                    'Reset so that a new Authorize will be required            
                    BCDifferenceData.TransactionId = strAuthTxn

                    If ChkMultiplePartialCapture.Checked Then
                        'Let's demonstrate capturing two multipartialCaptures. Note that multipartialCapture requries the BankcarCapturePro() object
                        BCDifferenceData.Amount = 5D
                        BCDifferenceData.MultiplePartialCapture = True

                        Helper.ProcessBCPTransaction(TransactionType.Capture, Nothing, BCDifferenceData, Nothing, Nothing, Nothing, _
                         Nothing, Nothing, Nothing, ChkAcknowledge.Checked, False)
                        Helper.ProcessBCPTransaction(TransactionType.Capture, Nothing, BCDifferenceData, Nothing, Nothing, Nothing, _
                         Nothing, Nothing, Nothing, ChkAcknowledge.Checked, False)
                    Else
                        BCDifferenceData.Amount = 12D
                        BCDifferenceData.TipAmount = 2D

                        BCDifferenceData.MultiplePartialCapture = False
                        Helper.ProcessBCPTransaction(TransactionType.Capture, Nothing, BCDifferenceData, Nothing, Nothing, Nothing, _
                         Nothing, Nothing, Nothing, ChkAcknowledge.Checked, False)
                    End If
