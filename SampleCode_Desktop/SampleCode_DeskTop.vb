        Private Sub cmdGo_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdGo.Click
            If txtServiceKey.Text.Length < 1 And chkDedicated.Checked Then
                MessageBox.Show("Please enter a valid Service Key")
                Return
            End If
            If txtIdentityToken.Text.Length < 1 Then
                MessageBox.Show("Please enter a valid Identity Token")
                Return
            End If

            Helper.ServiceKey = txtServiceKey.Text
            Helper.IdentityToken = txtIdentityToken.Text

            DefaultSetupValues()

            If chkDedicated.Checked Then
                'Set the Primary and Secondary Endpoints
                txtSvcPrimary.Text = Convert.ToString(Helper.BaseSvcEndpointPrimary) & "/" & Convert.ToString(Helper.ServiceKey)
                txtSvcSecondary.Text = Convert.ToString(Helper.BaseSvcEndpointSecondary) & "/" & Convert.ToString(Helper.ServiceKey)
                txtTxnPrimary.Text = Convert.ToString(Helper.BaseTxnEndpointPrimary) & "/" & Convert.ToString(Helper.ServiceKey)
                txtTxnSecondary.Text = Convert.ToString(Helper.BaseTxnEndpointSecondary) & "/" & Convert.ToString(Helper.ServiceKey)
            End If
            If chkDynamic.Checked Then
                'Set the Primary and Secondary Endpoints
                txtSvcPrimary.Text = Helper.BaseSvcEndpointPrimary
                txtSvcSecondary.Text = Helper.BaseSvcEndpointSecondary
                txtTxnPrimary.Text = Helper.BaseTxnEndpointPrimary
                txtTxnSecondary.Text = Helper.BaseTxnEndpointSecondary
            End If

            If Helper.ServiceKey.Length > 0 AndAlso LoadPersistedConfig() Then
                _blnPersistedConfigExists = True
                If Helper.ApplicationProfileId.Length > 0 AndAlso Helper.ServiceID.Length > 0 AndAlso Helper.IdentityToken.Length > 0 Then
                    Dim Result As DialogResult
                    Result = MessageBox.Show("Since you've previously persisted ApplicationProfileId, MerchantProfileId and ServiceId would you like to go directly to 'Transaction Processing'?", "Continue to Transaction Processing", MessageBoxButtons.YesNo)
                    If Result = DialogResult.Yes Then
                        tabControl1.SelectedTab = tbTransactionProcessing
                    Else
                        tabControl1.SelectedTab = tbPreparingApplicationToTransact
                    End If
                Else
                    tabControl1.SelectedTab = tbPreparingApplicationToTransact
                    cmdSignOnWithToken.Enabled = True
                    Return
                End If

                If Not Helper.SetSvcEndpoint() Then
                    MessageBox.Show("Unable to setup the service endpoint")
                End If
                If Not Helper.SetTxnEndpoint() Then
                    MessageBox.Show("Unable to setup the Transaction endpoint")
                End If
                If Not Helper.SetTMSEndpoint() Then
                    MessageBox.Show("Unable to setup the TMS endpoint")
                End If

                cmdSignOnWithToken.Enabled = True
                cmdSaveApplicationConfiguration.Enabled = True
                cmdRetrieveServiceInformation.Enabled = True

                'Perform Step 1 as a previous configuration was already saved
                Helper.SessionToken = Helper.Cwssic.SignOnWithToken(Helper.IdentityToken.Trim())
                chkStep1.Checked = True

                'Check to see if a previously saved ApplicationProfileId exists. 
                If Helper.ServiceID.Length > 1 Then
                    'Perform Step 3 as a previous configuration was already saved
                    GetServiceInformation()
                    Try
                        For Each i As Item In cboAvailableServices.Items
                            If i.Value1 = Helper.ServiceID Then
                                cboAvailableServices.SelectedItem = i
                                Exit For
                            End If
                        Next
                        chkStep3.Checked = True
                        cmdPersistConfig.Enabled = True
                        cmdDeletePersistCached.Enabled = True
                    Catch

                    End Try
                End If
                If Helper.MerchantProfileId.Length > 1 Or Helper.MerchantProfileId = "" Then
                    'Perform Step 4 as a previous configuration was already saved
                    GetMerchantProfileIds()
                    Try
                        'cboAvailableProfiles.SelectedItem = _ProfileId;
                        For Each i As Item In cboAvailableProfiles.Items
                            If i.Value1 = Helper.MerchantProfileId Then
                                cboAvailableProfiles.SelectedItem = i
                                Exit For
                            End If
                        Next
                        cmdPersistConfig.Enabled = True
                        cmdDeletePersistCached.Enabled = True
                    Catch
                    End Try
                End If

                Dim strEncryptedIdentityToken As String = vbCr & vbLf & "Identity Token [NOT ENCRYPTED]"
                If _blnEncryptedIdentityToken Then
                    strEncryptedIdentityToken = vbCr & vbLf & "Identity Token [ENCRYPTED]"
                    _blnEncryptedIdentityToken = True
                End If
                txtPersistedAndCached.Text = "ApplicationProfileId : " & Convert.ToString(Helper.ApplicationProfileId) & vbCr & vbLf & "ServiceId : " & Convert.ToString(Helper.ServiceID) & vbCr & vbLf & "MerchantProfileId : " & Convert.ToString(Helper.MerchantProfileId) & strEncryptedIdentityToken
                ' set back to false so the form behaves as normal. 
                _blnPersistedConfigExists = False
            Else
                tabControl1.SelectedTab = tbPreparingApplicationToTransact
                cmdSignOnWithToken.Enabled = True
            End If
        End Sub

