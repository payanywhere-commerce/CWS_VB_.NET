        Private Sub cmdPersistConfig_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdPersistConfig.Click
            'NOTE
            '             * Special consideration is necessary for protecting the identity token. You are required to be familure with the following
            '             * information regarding the identity token.
            '             * https://mylab.ipcommerce.com/Docs/TransactionProcessing/CWS/Implementation_Guidelines/2.0.17/ServiceInformationGuidelines/AuthenticationProcess/index.aspx
            '            


            Dim Result As DialogResult
            'Verify values to persist
            If Helper.ApplicationProfileId.Length < 1 Or Helper.ServiceID.Length < 1 Or Helper.MerchantProfileId.Length < 1 Then
                Dim sMessage As String = "Missing value(s) for" & vbCr & vbLf
                If Helper.ApplicationProfileId.Length < 1 Then
                    sMessage = sMessage & " - ApplicationProfileId" & vbCr & vbLf
                End If
                If Helper.ServiceID.Length < 1 Then
                    sMessage = sMessage & " - ServiceID" & vbCr & vbLf
                End If
                If Helper.MerchantProfileId.Length < 1 Then
                    sMessage = sMessage & " - MerchantProfileId" & vbCr & vbLf
                End If
                Result = MessageBox.Show(sMessage & vbCr & vbLf & "Continue?", "Verify Values to Save", MessageBoxButtons.YesNo)
                If Result <> DialogResult.Yes Then
                    Return
                End If
            End If

            Dim strIdentityToken As String = ""
            Dim blnEncryptedIdentityToken As Boolean = False
            If Not chkEncryptIdentityToken.Checked Then
                MessageBox.Show("Since the identity token is not going to be encrypted, the " & "appilcation must securly protect the identity token with administrator file based securty")
                'The identity token will be stored as clear text
                strIdentityToken = txtIdentityToken.Text
            Else
                strIdentityToken = Helper.Encrypt(txtIdentityToken.Text)
                blnEncryptedIdentityToken = True
            End If
            _blnEncryptedIdentityToken = False
            Dim strIdentityTokenMessage As String = vbCr & vbLf & vbTab & "Identity Token [NOT ENCRYPTED]"
            If chkEncryptIdentityToken.Checked Then
                strIdentityTokenMessage = vbCr & vbLf & vbTab & "Identity Token [ENCRYPTED]"
                _blnEncryptedIdentityToken = True
            End If
            MessageBox.Show("The following values will be persisted" & vbCr & vbLf & vbCr & vbLf & vbTab & "ApplicationProfielId : " & Convert.ToString(Helper.ApplicationProfileId) & vbCr & vbLf & vbTab & "ServiceId : " & Convert.ToString(Helper.ServiceID) & vbCr & vbLf & vbTab & "ProfileId : " & Convert.ToString(Helper.MerchantProfileId) & strIdentityTokenMessage)
            Dim PACS As New PersistAndCacheSettings(Helper.ApplicationProfileId, Helper.ServiceID, Helper.MerchantProfileId, blnEncryptedIdentityToken, strIdentityToken)
            SavePersistedConfig(PACS)
            LoadPersistedConfig()

            Result = MessageBox.Show("Configuration values successfully persisted. Continue to Transaction Processing", "Continue to Transaction Processing", MessageBoxButtons.YesNo)
            If Result = DialogResult.Yes Then
                tabControl1.SelectedTab = tbTransactionProcessing
            End If
        End Sub

        Private Sub cmdDeletePersistCached_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdDeletePersistCached.Click
            'Delete Persisted/Cached value
            'NOTE
            '              * SECURITY CONSIDERATIONS
            '              * Stored on file system with read/write permission for only the application/service and IT Administration
            '              * Stored in DB with read/write permission for only the application/service and IT Administration
            '            

            'Delete
            If File.Exists(Convert.ToString(Helper.ServiceKey) & "_TransactionProcessing.config") Then
                Dim Result As DialogResult
                Result = MessageBox.Show("Are you sure you want to delete " & Convert.ToString(Helper.ServiceKey) & "_TransactionProcessing.config?", "Delete File?", MessageBoxButtons.YesNo)
                If Result <> DialogResult.Yes Then
                    Return
                End If

                File.Delete(Convert.ToString(Helper.ServiceKey) & "_TransactionProcessing.config")
                MessageBox.Show("'" & Convert.ToString(Helper.ServiceKey) & "_TransactionProcessing.config' has been deleted")

                Helper.ApplicationProfileId = ""
                Helper.ServiceID = ""
                Helper.MerchantProfileId = ""
                DefaultSetupValues()
                cmdSignOnWithToken.Enabled = True
            Else
                MessageBox.Show("Unable to find or delete " & Convert.ToString(Helper.ServiceKey) & "_TransactionProcessing.config'")
            End If
            LoadPersistedConfig()
        End Sub

