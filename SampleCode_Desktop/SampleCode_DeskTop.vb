        Private Sub cmdPurchaseCardL2_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdPurchaseCardL2.Click
            Cursor = Cursors.WaitCursor

            If _bcs IsNot Nothing Then
                'Process a BankCard Transaction
                'NOTE: The first step with purchase card level 2 is to submit a Authorize to verify that the card is a valid purchase card
                'Check to see if this the service is a host capture or terminal capture solution. This will drive logice in terms of useing an AuthorizeAndCapture Versus Authorize to Capture
                If _bcs.Tenders.CreditAuthorizeSupport = CreditAuthorizeSupportType.AuthorizeOnly Then
                    'In this case it's a Host capture so either an Authorize followed by capture or AuthorizeAndCapture should be used.
                    If Not SupportedTxnTypes.Authorize AndAlso Not SupportedTxnTypes.AuthAndCapture Then
                        MessageBox.Show("Purchase card requires support for Authorize or AuthorizeAndCapture")
                        Cursor = Cursors.[Default]
                        Return
                        'In this case its a terminal capture service which requires an Authorize followed by a captureall or capture selective
                    ElseIf Not SupportedTxnTypes.Authorize Or Not SupportedTxnTypes.CaptureAll Then
                        MessageBox.Show("Purchase card requires support for Authorize as well as CaptureAll or CaptureSelective")
                        Cursor = Cursors.[Default]
                        Return
                    End If
                End If

                Try
                    Dim BCtransaction As New BankcardTransactionPro()
                    Dim response As New List(Of Response)()


                    'First Check to see if this work flow is an "AuthorizeAndCapture" or an "Authorize" followed by a "Capture".
                    If chkL2AuthAndCapture.Checked AndAlso _bcs.Tenders.CreditAuthorizeSupport = CreditAuthorizeSupportType.AuthorizeOnly Then
                        'In this case the work flow is a "AuthorizeAndCapture"
                        Dim BCPO As New BankCardProProcessingOptions(PurchaseCardLevel.Level2, False, True)
                        BCtransaction = SetBankCardTxnData(BCPO)
                        Helper.ProcessBCPTransaction(TransactionType.AuthorizeAndCapture, BCtransaction, Nothing, Nothing, Nothing, Nothing, _
       Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                    ElseIf Not chkL2AuthAndCapture.Checked Then
                        'In this case the work flow is an "Authorize" followed by a "Capture"
                        Dim BCPO As New BankCardProProcessingOptions(PurchaseCardLevel.Level2, False, False)
                        BCtransaction = SetBankCardTxnData(BCPO)

                        response = Helper.ProcessBCPTransaction(TransactionType.Authorize, BCtransaction, Nothing, Nothing, Nothing, Nothing, _
                         Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                        If response.Count < 1 Then
                            Return
                        End If
                        Dim BCR As BankcardTransactionResponsePro = DirectCast(response(0), BankcardTransactionResponsePro)

                        'Note : Now let's send the capture for the level 2 transaction. 
                        'First verify that the card is a valid Purchase Card. 
                        If BCR.CommercialCardResponse = CommercialCardResponse.BusinessCard Or BCR.CommercialCardResponse = CommercialCardResponse.CorporateCard Or BCR.CommercialCardResponse = CommercialCardResponse.PurchaseCard Then
                            Dim BCP As New BankcardCapturePro()
                            BCP.Level2Data = SetLevel2Data()
                            BCP.TransactionId = BCR.TransactionId
                            'Set the transactionId to the original Authorize
                            Helper.ProcessBCPTransaction(TransactionType.Capture, Nothing, BCP, Nothing, Nothing, Nothing, _
                             Nothing, Nothing, Nothing, ChkAcknowledge.Checked, False)
                        Else
                            MessageBox.Show("Capture failed as Card PAN was not a valid Level 2 or Level 3 purchase Card")
                        End If
                    Else
                        MessageBox.Show("Unable to process purchase Card as the transaction types requested are not supported. Specifically AuthorizeAndCapture")
                    End If
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]

                End Try
            ElseIf _ecks IsNot Nothing Then
                'Process as a Check transaction
                Try
                    MessageBox.Show("Placeholder for ECK code. Please ask your solution consultant for an example")
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            End If
        End Sub

        Private Sub cmdPurchaseCardL3_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdPurchaseCardL3.Click
            Cursor = Cursors.WaitCursor
            'NOTE: The first step with purchase card level 2 is to submit a Authorize to verify that the card is a valid purchase card
            'Check to see if this transaction type is supported
            If Not SupportedTxnTypes.Authorize Or Not SupportedTxnTypes.Capture Then
                MessageBox.Show("Purchase card requires support for Authorize as well as Capture")
                Cursor = Cursors.[Default]
                Return
            End If

            If _bcs IsNot Nothing Then
                'Process a BankCard Transaction
                Try
                    Dim BCtransaction As New BankcardTransactionPro()
                    Dim response As New List(Of Response)()

                    'First Check to see if this work flow is an "AuthorizeAndCapture" or an "Authorize" followed by a "Capture".
                    If chkL3AuthAndCapture.Checked Then
                        'In this case the work flow is a "AuthorizeAndCapture"
                        Dim BCPO As New BankCardProProcessingOptions(PurchaseCardLevel.Level3, False, True)
                        BCtransaction = SetBankCardTxnData(BCPO)
                        Helper.ProcessBCPTransaction(TransactionType.AuthorizeAndCapture, BCtransaction, Nothing, Nothing, Nothing, Nothing, _
                         Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                    Else
                        'In this case the work flow is an "Authorize" followed by a "Capture"
                        Dim BCPO As New BankCardProProcessingOptions(PurchaseCardLevel.Level3, False, False)
                        BCtransaction = SetBankCardTxnData(BCPO)

                        response = Helper.ProcessBCPTransaction(TransactionType.Authorize, BCtransaction, Nothing, Nothing, Nothing, Nothing, _
                         Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                        If response.Count < 1 Then
                            Return
                        End If
                        Dim BCR As BankcardTransactionResponsePro = DirectCast(response(0), BankcardTransactionResponsePro)

                        'Note : Now let's send the capture for the level 3 transaction. 
                        'First verify that the card is a valid Purchase Card. 
                        If BCR.CommercialCardResponse = CommercialCardResponse.BusinessCard Or BCR.CommercialCardResponse = CommercialCardResponse.CorporateCard Or BCR.CommercialCardResponse = CommercialCardResponse.PurchaseCard Then
                            Dim BCP As New BankcardCapturePro()
                            BCP.Level2Data = SetLevel2Data()
                            'Level 3 purchase card is always inclusive of Level 1 and Level 2 data.
                            Dim LIDS As New List(Of LineItemDetail)()
                            'For each line on the receipt the following needs to be called. 
                            For i As Integer = 0 To 2
                                'Add three line items
                                LIDS.Add(SetLevel3Data())
                            Next
                            BCP.LineItemDetails = LIDS
                            BCP.TransactionId = BCR.TransactionId
                            'Set the transactionId to the original Authorize
                            Helper.ProcessBCPTransaction(TransactionType.Capture, Nothing, BCP, Nothing, Nothing, Nothing, _
                             Nothing, Nothing, Nothing, ChkAcknowledge.Checked, False)
                        Else
                            MessageBox.Show("Capture failed as Card PAN was not a valid Level 2 or Level 3 purchase Card")
                        End If
                    End If
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            ElseIf _ecks IsNot Nothing Then
                'Process as a Check transaction
                Try
                    MessageBox.Show("Placeholder for ECK code. Please ask your solution consultant for an example")
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            End If
