                    MessageBox.Show("Unable to RetrieveServiceInformation" & vbCr & vbLf & "Error Message : " & ex.Message, "RetrieveServiceInformation Failed", MessageBoxButtons.OK, MessageBoxIcon.Warning)
                End Try
            Catch ex As Exception
