        Private Sub cmdCaptureSelective_Click(ByVal sender As Object, ByVal e As EventArgs) Handles cmdCaptureSelective.Click
            'The CaptureSelective() operation is used to capture a specific list of transactions that have been successfully authorized.
            Cursor = Cursors.WaitCursor

            'Check to see if this transaction type is supported
            If Not SupportedTxnTypes.CaptureSelective Then
                MessageBox.Show("CaptureSelective Not Supported")
                Cursor = Cursors.[Default]
                Return
            End If

            If _bcs IsNot Nothing Then
                'Process a BankCard Transaction
                Try
                    Dim transactionIDs As New List(Of String)()
                    Dim captures As New List(Of Capture)()
                    Dim response As New List(Of Response)()
                    Dim BCCSE As New List(Of BankCardCaptureSelectiveExample)()
                    'Only used for demonstration
                    'Note : Illustrate the effect of a ReturnById with a subsequent Settlement.
                    Dim BCtransactionForAReturn As BankcardTransaction = SetBankCardTxnData()
                    BCtransactionForAReturn.TransactionData.Amount = 22D
                    response = Helper.ProcessBCPTransaction(TransactionType.Authorize, BCtransactionForAReturn, Nothing, Nothing, Nothing, Nothing, _
                     Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                    If response.Count < 1 Then
                        Return
                    End If
                    Dim BCR As BankcardTransactionResponse = DirectCast(response(0), BankcardTransactionResponse)
                    Dim strReturnTransactionId As String = BCR.TransactionId
                    transactionIDs.Add(BCR.TransactionId)
                    If chkCaptureSelectiveAsync.Checked Then
                        Helper.ProcessBCPTransaction(TransactionType.CaptureSelectiveAsync, Nothing, Nothing, Nothing, Nothing, Nothing, _
       Nothing, transactionIDs, Nothing, ChkAcknowledge.Checked, False)
                    Else
                        Helper.ProcessBCPTransaction(TransactionType.CaptureSelective, Nothing, Nothing, Nothing, Nothing, Nothing, _
                         Nothing, transactionIDs, Nothing, ChkAcknowledge.Checked, False)
                    End If
                    transactionIDs.Clear()
                    'Reset the transactions for the second settlement.
                    response.Clear()

                    'Note : First build up a set of Authorize transactions to batch capture                
                    Dim intNumberOfAutorizeTxn As Integer = 3
                    Dim intcount As Integer = 0
                    Dim amount As Integer = 10
                    While intcount < intNumberOfAutorizeTxn
                        Dim BCtransaction As BankcardTransaction = SetBankCardTxnData()
                        BCtransaction.TransactionData.Amount = Convert.ToDecimal((amount).ToString("0.00"))


                        amount += 1
                        'Increment to keep transactons unique
                        response = Helper.ProcessBCPTransaction(TransactionType.Authorize, BCtransaction, Nothing, Nothing, Nothing, Nothing, _
       Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                        If response.Count < 1 Then
                            Return
                        End If
                        BCR = DirectCast(response(0), BankcardTransactionResponse)
                        BCCSE.Add(New BankCardCaptureSelectiveExample(response(0).TransactionId, BCR.Amount, False, True))
                        intcount += 1
                    End While

                    'Note : Let's add a PINDebit Transaction
                    Dim BCtransaction_PIN As BankcardTransaction = SetBankCardTxnData()
                    If BCtransaction_PIN.TransactionData.IndustryType = IndustryType.Retail Or BCtransaction_PIN.TransactionData.IndustryType = IndustryType.Restaurant Then
                        'Since PINDebit is only supported for card present such as Retail and Restaurant, only process example if Retail or Restaurant
                        BCtransaction_PIN.TenderData.CardSecurityData = New CardSecurityData()
                        BCtransaction_PIN.TenderData.CardSecurityData.KeySerialNumber = "1234567890123456"
                        BCtransaction_PIN.TenderData.CardSecurityData.PIN = "1234567890"
                        BCtransaction_PIN.TransactionData.Amount = 15D
                        BCtransaction_PIN.TransactionData.AccountType = AccountType.CheckingAccount
                        BCtransaction_PIN.TransactionData.InvoiceNumber = "123"
                        response = Helper.ProcessBCPTransaction(TransactionType.AuthorizeAndCapture, BCtransaction_PIN, Nothing, Nothing, Nothing, Nothing, _
                         Nothing, Nothing, Nothing, ChkAcknowledge.Checked, ChkUserWorkflowId.Checked)
                        If response.Count < 1 Then
                            Return
                        End If
                        BCR = DirectCast(response(0), BankcardTransactionResponse)
                        BCCSE.Add(New BankCardCaptureSelectiveExample(BCR.TransactionId, BCR.Amount, True, False))
                    End If

                    'Note : Let's add a ReturnById to the settlement.
                    Dim R2 As New BankcardReturn()
                    R2.TransactionId = strReturnTransactionId
                    response = Helper.ProcessBCPTransaction(TransactionType.ReturnById, Nothing, Nothing, Nothing, R2, Nothing, _
      Nothing, Nothing, Nothing, ChkAcknowledge.Checked, False)
                    If response.Count < 1 Then
                        Return
                    End If
                    BCR = DirectCast(response(0), BankcardTransactionResponse)
                    transactionIDs.Add(BCR.TransactionId)


                    'Note : Now build the batch capture transactions
                    Dim blnProcessVoidInstead As Boolean = True
                    Dim BCDifferenceData As New BankcardCapture()

                    For Each _BCCSE As BankCardCaptureSelectiveExample In BCCSE
                        'Note : Let's add one Undo-void to the mix
                        If blnProcessVoidInstead And Not _BCCSE.PinDebitTxn Then
                            Dim uTransaction As New BankcardUndo()
                            uTransaction.TransactionId = _BCCSE.TransactionId
                            'Now Let's Undo or Void the transaction
                            response = Helper.ProcessBCPTransaction(TransactionType.Undo, Nothing, Nothing, Nothing, Nothing, Nothing, _
                             uTransaction, Nothing, Nothing, ChkAcknowledge.Checked, False)
                            If response.Count < 1 Then
                                Return
                            End If
                            BCR = DirectCast(response(0), BankcardTransactionResponse)
                            'VOID transactionId's are not added to the Batch settlement.
                            'Only process one
                            blnProcessVoidInstead = False
                        ElseIf Not _BCCSE.PinDebitTxn Then
                            'Note that PINDebit transaction are not allowed as part of the difference data

                            BCDifferenceData.TransactionId = _BCCSE.TransactionId
                            BCDifferenceData.ShipDate = DateTime.Now
                            'If not passed a default value will be used. This value will show on daily reports.
                            transactionIDs.Add(_BCCSE.TransactionId)
                            'The following required for Retail
                            BCDifferenceData.ChargeType = ChargeType.RetailOther
                            'Add tip if applies
                            If _BCCSE.TippAmt Then
                                BCDifferenceData.Amount = 2D + _BCCSE.Amount
                                BCDifferenceData.TipAmount = 2D
                            End If
                            captures.Add(BCDifferenceData)
                        End If
                    Next
                    If chkCaptureSelectiveAsync.Checked Then
                        Helper.ProcessBCPTransaction(TransactionType.CaptureSelectiveAsync, Nothing, Nothing, Nothing, Nothing, Nothing, _
                         Nothing, transactionIDs, captures, ChkAcknowledge.Checked, False)
                    Else
                        Helper.ProcessBCPTransaction(TransactionType.CaptureSelective, Nothing, Nothing, Nothing, Nothing, Nothing, _
                         Nothing, transactionIDs, captures, ChkAcknowledge.Checked, False)
                    End If
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            ElseIf _ecks IsNot Nothing Then
                'Process as a Check transaction
                Try
                    MessageBox.Show("Placeholder for ECK code. Please ask your solution consultant for an example")
                Catch ex As Exception
                    MessageBox.Show(ex.Message)
                Finally
                    Cursor = Cursors.[Default]
                End Try
            End If
        End Sub

