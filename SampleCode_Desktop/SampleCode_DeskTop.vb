            'If Convert.ToBoolean(ConfigurationSettings.AppSettings("TxnData_ProcessMagensaTxn")) And (supportedTxnTypes.Authorize Or supportedTxnTypes.AuthAndCapture) Then
            '    CmdMagensa.Enabled = True
            'End If

            If supportedTxnTypes.AuthAndCapture Then
                cmdAuthorizeAndCapture.Enabled = True
                cmdForcePost.Enabled = True
            End If
            If supportedTxnTypes.Authorize Then
                cmdAuthorize.Enabled = True
            End If
            If supportedTxnTypes.Capture Then
                cmdCapture.Enabled = True
            End If
            If supportedTxnTypes.CaptureAll Then
                cmdCaptureAll.Enabled = True
                chkCaptureAllAsync.Enabled = True
            End If
            If supportedTxnTypes.CaptureSelective Then
