            End If
            If _svas IsNot Nothing Then
                'Process a Stored Value Transaction
                Try
                    Dim SVtransaction As StoredValueTransaction = SetStoredValueTxnData()
                    Dim response As New List(Of Response)()
                    Dim uTransaction As New Undo()

                    'Let's authorize a transaction
                    response = Helper.ProcessSVATransaction(TransactionType.Authorize, SVtransaction, Nothing, Nothing, Nothing, Nothing, _
                     ChkAcknowledge.Checked)
                    Dim SVR As StoredValueTransactionResponse = DirectCast(response(0), StoredValueTransactionResponse)
